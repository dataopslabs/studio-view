# Application Configuration
ENVIRONMENT=development
DEBUG=true
API_TITLE=AI QA Automation Platform
API_VERSION=1.0.0

# Google Cloud Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
GEMINI_MODEL=gemini-1.5-pro-vision
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=2048

# Database Configuration
DATABASE_URL=postgresql://qa_user:qa_password@localhost:5432/qa_automation
DB_USER=qa_user
DB_PASSWORD=qa_password
DB_NAME=qa_automation
DB_HOST=localhost
DB_PORT=5432

# Google DocumentAI Configuration
DOCAI_PROJECT_ID=your_gcp_project_id
DOCAI_LOCATION=us
DOCAI_PROCESSOR_ID=your_processor_id

# Google Cloud Storage
GCS_BUCKET=your_gcs_bucket_name
GCS_VIDEO_FOLDER=videos
GCS_TEMP_FOLDER=temp

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:8000
CORS_CREDENTIALS=true
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_HEADERS=Content-Type,Authorization

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_ENVIRONMENT=development

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Feature Flags
ENABLE_VIDEO_UPLOAD=true
ENABLE_ADK_EXECUTION=true
ENABLE_VALIDATION=true
ENABLE_DOCAI_EXTRACTION=true

# Execution Configuration
EXECUTION_TIMEOUT=300
MAX_VIDEO_SIZE=5120000000
ALLOWED_VIDEO_FORMATS=mp4,avi,mov,mkv
